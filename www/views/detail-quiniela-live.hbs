
	<div id="detailQuinielaRegistered" data-id="{{data.id}}" data-entry="{{data.entry_id}}" class="content quiniela_single quiniela_registrada_single">
		
		<a class="link_wrapper">
			<div class="quiniela {{# if data.upcoming}}quiniela_open{{/ if}} 
			{{# if data.live}}quiniela_live{{/ if}} 
			{{# if_eq data.status 'live'}}quiniela_live{{/ if_eq}} 
			{{# if data.closed}}quiniela_closed{{/ if}} {{data.sport.name}}
			{{# if_eq data.point_format "natural" }} natural {{/ if_eq }}  clearfix">
				<div class="quiniela_header clearfix">
					<div class="quiniela_iconos clearfix">
						{{# if_eq data.point_format "line" }}
							<img src="{{cordova_full_path}}images/linea.png">
						{{/ if_eq }}
						{{# if_eq data.point_format "natural" }}
							<img src="{{cordova_full_path}}images/natural.png">
						{{/ if_eq }}
						<img src="{{cordova_full_path}}images/{{data.sport.icon}}.png">
					</div><!-- iconos -->
					<h2>{{data.name_replaced}}</h2>
				</div><!-- quiniela_header -->
				<div class="quiniela_content clearfix">
					<div class="quiniela_info clearfix">
						<span class="registros">REG: {{data.entry_count}}</span>
						<span class="cierre">{{^ if data.closed}}CIERRE{{/ if}}{{# if data.closed}}CERRÓ{{/ if}}: {{formatDate data.deadline_tz 'lll'}}</span>
						{{# data.limited_capacity}}
							<span class="limite">Limitado a {{data.max_entries}} entradas</span>
						{{/ data.limited_capacity}}
						{{# data.closed}}
							<span class="limite">FINALIZÓ</span>
						{{/ data.closed}}
						{{# if_eq status "live"}}
							<span class="live">JUGANDO</span>
						{{/ if_eq}}
					</div><!-- quiniela_info -->
					
					<div class="quiniela_entrada clearfix">
						<span class="titulo">ENTRADA</span>
						{{# if_eq data.entry_currency 'real' }}
							<img src="{{cordova_full_path}}images/billecoin.png">
						{{/ if_eq }}
						{{# if_eq data.entry_currency 'pickcoin' }}
							<img src="{{cordova_full_path}}images/pickcoin.png">
						{{/ if_eq }}
						<span class="number">${{ formatCurrency data.entry_fee}}</span>
					</div>
					<div class="quiniela_bolsa clearfix">
						<span class="titulo">BOLSA</span>
						{{# if data.guaranteed_prize}}
							<img src="{{cordova_full_path}}images/garantizado.png">
						{{/ if }}
						<span class="number">${{ formatCurrency data.available}}</span>
					</div>
				</div><!-- quiniela_content -->
			</div><!-- quiniela -->
		</a><!-- link_wrapper -->
		<div id="lesDrops" class="quinielas_dropdowns live clearfix">
			
	
		</div><!-- dropdowns -->
		<div class="tabs_quiniela clearfix">
			<ul>
				<li class="selected"><a class="hook missing_info" data-object="{{data.id}}" data-extra="" data-resource="detail-postures" href="detail.html?registered=true&tab=postures">Picks</a></li>
				<li><a class="hook missing_info" data-object="{{data.id}}" data-extra="" data-resource="detail-places" href="detail.html?registered=true&tab=places">Posiciones</a></li>
				<li><a class="hook missing_info" data-object="{{data.id}}" data-extra="" data-resource="detail-chat" href="detail.html?registered=true&tab=chat">Chat</a></li>
				<li><a class="hook missing_info" data-object="{{data.id}}" data-extra="" data-resource="detail-prizes" href="detail.html?registered=true&tab=prizes">Premios</a></li>
				<li><a class="hook missing_info" data-object="{{data.id}}" data-extra="" data-resource="detail-group-picks" href="detail.html?registered=true&tab=group-picks">Picks de grupo</a></li>
				<li><a class="hook missing_info" data-object="{{data.id}}" data-extra="" data-resource="detail-scores" href="detail.html?registered=true&tab=scores">Marcadores</a></li>
			</ul>
		</div>

		<div id="tabContainer" class="tabs">
			<div id="insertPartidos" class="partidos clearfix radio_group">
				
			</div><!-- partidos -->
				<div class="desempates clearfix">
					<h3>DESEMPATE</h3>
						{{# each data.tiebreakers}}
						tie
							{{# if_eq type 'score'}}
								<div class="desempate desempate_uno clearfix">
									
									<div class="desempate_left">
										<span>{{label}}</span>
									</div>
									<div class="desempate_input">
										<span>LOC</span>
										<input type="tel" name="desempate_uno_local">
										<span>-</span>
										<input type="tel" name="desempate_uno_visitante">
										<span>VIS</span>
									</div><!-- desempate_input -->
								
								</div><!-- desempate_uno -->
							{{/ if_eq }}

							{{# if_eq type 'number'}}
								<div class="desempate desempate_dos clearfix">
									<div class="desempate_left">
										<span>{{label}}</span>
									</div>
									<div class="desempate_input">
										<input type="tel" name="desempate_dos_minuto">
									</div><!-- desempate_input -->
								</div><!-- desempate_dos -->
							{{/ if_eq }}
							
							{{# if_eq type 'week_goals'}}
								<div class="desempate desempate_dos clearfix">
									<div class="desempate_left">
										<span>{{label}}</span>
									</div>
									<div class="desempate_input">
										<input type="tel" name="desempate_dos_minuto">
									</div><!-- desempate_input -->
								</div><!-- desempate_dos -->
							{{/ if_eq }}

							{{# if_eq type 'other'}}
								<div class="desempate desempate_tres clearfix">
									<div class="desempate_left">
										<span>{{label}}</span>
									</div>
									<div class="desempate_input">
										<span>SI</span>
										<input type="radio" name="desempate_tres" value="si">
										<span>NO</span>
										<input type="radio" name="desempate_tres" value="no">
									</div><!-- desempate_input -->
								</div><!-- desempate_tres -->
							{{/ if_eq }}
						{{/ each}}

				</div><!-- dempates -->
		
		</div>
		
		
	</div><!-- content -->
	