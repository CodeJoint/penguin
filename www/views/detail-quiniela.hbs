							</div><!-- total dinero -->
							<div class="total_coins">
								<img src="{{cordova_full_path}}images/pickcoin.png">
								<p>0 + 0 comisión</p>
							</div><!-- total coins -->
						</div><!-- total -->
					</div><!-- balance -->

					<div class="botones clarfix">
						<a><img src="{{cordova_full_path}}images/regresar.png"></a>
						<input type="submit">
					</div>
				</div><!-- quiniela-registro -->

			</div><!-- quiniela_registro_overlay -->
