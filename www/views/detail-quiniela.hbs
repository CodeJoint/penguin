
	<div class="container clearfix">
		{{> header}}
		<div class="content quiniela_single clearfix">
			<div class="registro_quiniela clearfix">
				<a class="hook" data-resource="lobby" href="lobby.html"><img src="{{cordova_full_path}}images/cierre.png"></a>
				<a href=""><img src="{{cordova_full_path}}images/registrarse.png"></a>
			</div><!-- opciones registro -->
			<a class="link_wrapper" href="">
				<div class="quiniela
				{{# if data.upcoming}}quiniela_open{{/ if}} 
				{{# if data.live}}quiniela_live{{/ if}} 
				{{# if_eq data.status 'live'}}quiniela_live{{/ if_eq}} 
				{{# if data.closed}}quiniela_closed{{/ if}} {{data.sport.name}}
				{{# if_eq data.point_format "natural" }} natural {{/ if_eq }} garantizado clearfix">
					<div class="quiniela_header clearfix">
						<div class="quiniela_iconos clearfix">
							{{# if_eq data.point_format "line" }}
								<img src="{{cordova_full_path}}images/linea.png">
							{{/ if_eq }}
							{{# if_eq data.point_format "natural" }}
								<img src="{{cordova_full_path}}images/natural.png">
							{{/ if_eq }}
							<img src="{{cordova_full_path}}images/{{data.sport.icon}}.png">
						</div><!-- iconos -->
						<h2>{{name_replaced}}</h2>
					</div><!-- quiniela_header -->
					<div class="quiniela_content clearfix">

						<div class="quiniela_info clearfix">
							<span class="registros">REG: {{data.entry_count}}</span>
							<span class="cierre">CIERRE: {{formatDate data.deadline_tz}}</span>
							{{# data.limited_capacity}}
								<span class="limite">Limitado a {{data.max_entries}} entradas</span>
							{{/ data.limited_capacity}}
						</div><!-- quiniela_info -->
						<div class="quiniela_entrada clearfix">
							<span class="titulo">ENTRADA</span>
							{{# if_eq data.entry_currency 'real' }}
								<img src="{{cordova_full_path}}images/billecoin.png">
							{{/ if_eq }}
							{{# if_eq data.entry_currency 'pickcoin' }}
								<img src="{{cordova_full_path}}images/pickcoin.png">
							{{/ if_eq }}
							<span class="number">${{ formatCurrency data.entry_fee}}</span>
						</div>
						<div class="quiniela_bolsa clearfix">
							<span class="titulo">BOLSA</span>
							{{# if data.guaranteed_prize}}
								<img src="{{cordova_full_path}}images/garantizado.png">
							{{/ if }}
							<span class="number">${{ formatCurrency data.available}}</span>
						</div>
					</div><!-- quiniela_content -->
				</div><!-- quiniela -->
			</a><!-- link_wrapper -->

			<div class="quinielas_dropdowns clearfix">
				
					<a href="">Copiar picks</a>

				
					<a href="">Registros</a>
			
			</div><!-- dropdowns -->
			<div class="partidos clearfix">
				<h3>PARTIDOS</h3>
				<div class="opciones_partido clearfix">
					<span>LOCAL</span>
					<span>EMPATE</span>
					<span>VISITA</span>
				</div><!-- opciones_partido -->
				<div class="partido clearfix">
					<div class="opcion_partido local clearfix">
						<input type="radio" name="ganador_uno" id="local">
						<img src="{{cordova_full_path}}images/camisaverde.png">
						<div class="equipo">
							<span class="iniciales">ABC</span>
						</div><!-- equipo -->
						<div class="linea">
							<span>(+4) 123</span>
						</div><!-- linea -->
					</div><!-- opcion_partido -->
					<div class="opcion_partido cierre clearfix">
						<span>CIERRE: 28.feb.17</span>
					</div><!-- opcion_partido -->
					<div class="opcion_partido visitante clearfix">
						<input type="radio" name="ganador_uno" id="visitante">
						<img src="{{cordova_full_path}}images/camisaazul.png">
						<div class="equipo">
							<span class="iniciales">ABC</span>
						</div><!-- equipo -->
						<div class="linea">
							<span>(+4) 123</span>
						</div><!-- linea -->
					</div><!-- opcion_partido -->
				</div><!-- partido -->
				<div class="partido clearfix">
					<div class="opcion_partido local clearfix">
						<input type="radio" name="ganador_dos" id="local">
						<img src="{{cordova_full_path}}images/camisablanca.png">
						<div class="equipo">
							<span class="iniciales">ABC</span>
						</div><!-- equipo -->
						<div class="linea">
							<span>(+4) 123</span>
						</div><!-- linea -->
					</div><!-- opcion_partido -->
					<div class="opcion_partido empate cierre clearfix">
						<input type="radio" name="ganador_dos" id="empate">
						<span>CIERRE: 28.feb.17</span>
					</div><!-- opcion_partido -->
					<div class="opcion_partido visitante clearfix">
						<input type="radio" name="ganador_dos" id="visitante">
						<img src="{{cordova_full_path}}images/camisaazul.png">
						<div class="equipo">
							<span class="iniciales">ABC</span>
						</div><!-- equipo -->
						<div class="linea">
							<span>(+4) 123</span>
						</div><!-- linea -->
					</div><!-- opcion_partido -->
				</div><!-- partido -->
				<div class="partido clearfix">
					<div class="opcion_partido local clearfix">
						<input type="radio" name="ganador_tres" id="local">
						<img src="{{cordova_full_path}}images/camisaamarilla.png">
						<div class="equipo">
							<span class="iniciales">ABC</span>
						</div><!-- equipo -->
						<div class="linea">
							<span>(+4) 123</span>
						</div><!-- linea -->
					</div><!-- opcion_partido -->
					<div class="opcion_partido empate cierre clearfix">
						<input type="radio" name="ganador_tres" id="empate">
						<span>CIERRE: 28.feb.17</span>
					</div><!-- opcion_partido -->
					<div class="opcion_partido visitante clearfix">
						<input type="radio" name="ganador_tres" id="visitante">
						<img src="{{cordova_full_path}}images/camisaroja.png">
						<div class="equipo">
							<span class="iniciales">ABC</span>
						</div><!-- equipo -->
						<div class="linea">
							<span>(+4) 123</span>
						</div><!-- linea -->
					</div><!-- opcion_partido -->
				</div><!-- partido -->
			</div><!-- partidos -->
