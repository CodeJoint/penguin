	<div class="tab tab_premios clearfix">
		<table>
			<tr class="table_title">
				<th>LUGAR</th>
				<th>PORCENTAJE</th>
				<th>MONTO</th>
			</tr>
			{{# if prize_distribution.pool_prizes }}
				{{# each prizes }}
					<tr class="player_info">
						<td>{{position}} LUGAR</td>
						{{# if_eq type 'fixed'}}
							<td>{{calcPercentage amount ../available}}%</td>
							<td class="ganar"><img src="images/billete.png">${{formatCurrency amount}} MXN</td>
						{{/ if_eq }}
						{{# if_eq type 'percentage'}}
							<td>{{formatCurrency percentage}}%</td>
							<td class="ganar"><img src="images/billete.png">${{calcAmount percentage prize_distribution.pool.available}} MXN</td>
						{{/ if_eq }}
					</tr>
				el 1 ->{{prize_distribution.pool.available}}
				el ->{{../prize_distribution.pool.available}}
				{{/ each }}
			{{/ if }}
		</table>
	</div><!-- premios -->